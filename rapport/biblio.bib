@article{edwards_rapid_2025,
	title = {Rapid butterfly declines across the {United} {States} during the 21st century},
	volume = {387},
	url = {https://www.science.org/doi/10.1126/science.adp4671},
	doi = {10.1126/science.adp4671},
	abstract = {Numerous declines have been documented across insect groups, and the potential consequences of insect losses are dire. Butterflies are the most surveyed insect taxa, yet analyses have been limited in geographic scale or rely on data from a single monitoring program. Using records of 12.6 million individual butterflies from {\textgreater}76,000 surveys across 35 monitoring programs, we characterized overall and species-specific butterfly abundance trends across the contiguous United States. Between 2000 and 2020, total butterfly abundance fell by 22\% across the 554 recorded species. Species-level declines were widespread, with 13 times as many species declining as increasing. The prevalence of declines throughout all regions in the United States highlights an urgent need to protect butterflies from further losses.},
	number = {6738},
	urldate = {2025-04-20},
	journal = {Science},
	author = {Edwards, Collin B. and Zipkin, Elise F. and Henry, Erica H. and Haddad, Nick M. and Forister, Matthew L. and Burls, Kevin J. and Campbell, Steven P. and Crone, Elizabeth E. and Diffendorfer, Jay and Douglas, Margaret R. and Drum, Ryan G. and Fallon, Candace E. and Glassberg, Jeffrey and Grames, Eliza M. and Hatfield, Rich and Hershcovich, Shiran and Hoffman Black, Scott and Larsen, Elise A. and Leuenberger, Wendy and Linders, Mary J. and Longcore, Travis and Marschalek, Daniel A. and Michielini, James and Neupane, Naresh and Ries, Leslie and Shapiro, Arthur M. and Swengel, Ann B. and Swengel, Scott R. and Taron, Douglas J. and Van Deynze, Braeden and Wiedmann, Jerome and Thogmartin, Wayne E. and Schultz, Cheryl B.},
